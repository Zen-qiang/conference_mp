var statusConvert = function (value) {
  var str = ''
  switch (value) {
    case 0:
      str = '已取消'
      break
    case 1:
      str = '待支付'
      break
    case 2:
      str = '待发货'
      break
    case 3:
      str = '待收货'
      break
    case 4:
      str = '待评价'
      break
    case 5:
      str = '已评价'
      break
    case 6:
      str = '售后退款'
      break
    default:
      str = ''
      break
  }
  return str
}
module.exports = {
  statusConvert: statusConvert
}