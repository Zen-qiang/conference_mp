.manName {
width: 100%;
height: 100rpx;
line-height: 100rpx;
padding: 0 30rpx;
box-sizing: border-box;
background-color: #fff;
border-top: 1px solid #f2f2f2;
display: flex;
justify-content: space-between;
font-size: 28rpx;
}
.headImg {
  display: flex;
  padding-top: 15rpx;
  box-sizing: border-box;
}
.headImg view {
  width: 70rpx;
  height: 70rpx;
  margin-left: 5rpx;
  border-radius: 50%;
  background-color: red;
  position: relative;
}
.headImg view image {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  position: absolute;
  left: 0;
  right: 0;
}
.footCar {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color:#0cb4ce;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
  position: fixed;
  left: 0;
  bottom: 0;
}





