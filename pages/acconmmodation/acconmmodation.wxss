}

/* 酒店列表 */
.hotelList {
  width: 100%;
  padding-top: 600rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
  background-color: #f2f2f2;
}
.hotelBox {
  background-color: #fff;
  margin-bottom: 30rpx;
  border-bottom: 1px solid #ccc;
}
.mapIcon {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #f2f2f2;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
}
.mapImg {
  width: 64rpx;
  height: 64rpx;
  margin: auto 0;
}
.mapImg image {
  width: 64rpx;
  height: 64rpx;
}
.hotelInfo {
  padding: 30rpx;
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.leftImg {
  width: 180rpx;
  height: 130rpx;
  margin: auto 0;
  background-color: red;
}
.leftImg  image{
  width: 180rpx;
  height: 130rpx;
}
.rightText {
  font-size: 26rpx;
  color: #c6c6c6;
}
.textOne {
  margin-right: 10rpx;
}
.cardInfo {
  width: 100%;
}


