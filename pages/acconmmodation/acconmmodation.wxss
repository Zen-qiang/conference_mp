.acconmmodation {
  width: 100%;
  height: 100%;
  background-color: #f2f2f2;
}
.top {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  display: flex;
  border-top: 1px solid #f2f2f2;
  position: fixed;
  left: 0;
  top: 0;
}
.top view {
  width: 50%;
  text-align: center;
  font-size: 26rpx;
}
.top view text {
  display: inline-block;
  padding: 0 15rpx;
  box-sizing: border-box;
  height: 100rpx;
}

/* 安排入住 */
.card {
  width: 100%;
  position: fixed;
  left: 0;
  top: 103rpx;
  z-index: 9;
}
.kong {
  width: 100%;
  height: 30rpx;
  background-color: #f2f2f2;
}
.cardSelectop {
  margin-bottom: 30rpx;
  background-color: #fff;
  padding-bottom: 10rpx;
  box-sizing: border-box;
}
.fieldBox {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #000;
}
.van-cell {
  line-height: 65rpx !important;
}
