.signSuccess {}
/* 报名成功 */
.signSuccess .successInfo {
  text-align: center;
  margin: 1px 0;
  padding: 80rpx 0;
  background: #fff;
}
.successInfo .success-icon {
  color: #0eabc7;
  margin-bottom: 30rpx;
}
.successInfo .success-title {
  font-size: 44rpx;
}
.successInfo .success-subTitle {
  font-size: 24rpx;
  color: #999;
  margin-bottom: 30rpx;
}
.successInfo .success-qrCode {
  display: inline-block;
  padding: 10rpx;
  border: 1px solid #ddd;
  width: 100px;
  height: 100px;
}
.successInfo .success-qrCode image {
  width: 100%;
  height: 100%;
}
/* 成员信息 */
.signSuccess .signInfo {
  margin-bottom: 60rpx;
}
.signInfo .success-info {
  margin-bottom: 1px;
}
.signInfo .success-photo {}
/* 自定义van-cell组件 样式 */
.customCell {
  align-items: center;
}
.success-photo .member-content {}
.success-photo .member-content image {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.success-photo .member-content image + image {
  margin-left: 10rpx;
}
/* 安排行程 */
.signSuccess .planJourney {
  padding: 0 30rpx;
}
.planJourney .myBtn {
  width: 100%;
  border-radius: 22px;
}