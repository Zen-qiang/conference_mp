page {
  padding: 20rpx 0 60px;
}
.toLogin {
  padding-top: 300rpx;
  text-align: center;
}
.toLogin .toLoginBtn {
  padding: 0 150rpx;
  font-weight: bolder;
}
.list-box, .status-box {
  position: relative;
  z-index: 1;
}
.status-box {
  margin-top: 30rpx;
}
.submit-box {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 50px;
  z-index: 3;
  display: flex;
  background-color: #fff;
  flex-flow: row nowrap;
  justify-content: flex-end;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.submit-box .content {
  flex: 1;
  text-align: right;
  padding-right: 10px;
}
.submit-box .content .price {
  font-size: 32rpx;
  color: #ec132d;
  font-weight: 400;
}
.submit-box .content .price .currency {
  font-size: 24rpx;
}
.submit-box .content .price .small {
  font-size: 24rpx;
}
.submit-box .deleteSelectedItem {
  margin-left: 30rpx;
  font-size: 28rpx;
  color: #999;
  padding: 16rpx 20rpx;
  margin-left: 20rpx;
}
.submit-box .btn {
  width: 100px;
}
.submit-box .btn .btn-submit {
  width: 100%;
}