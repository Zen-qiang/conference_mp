.err {
  color: #f44;
}
.addMember {
  text-align: center;
  font-size: 28rpx;
  background: #fff;
  margin-top: 2px;
  margin-bottom: 4px;
  padding: 20rpx 0;
}
.addMember view {
  display: inline-block;
  color: #f44;
}
.addMember view text {
  display: inline-block;
  margin-left: 20rpx;
  vertical-align: middle;
  color: #999;
}
.memberContainer {
  padding-bottom: 100rpx;
}
.memberInfo {
  font-size: 24rpx;
  margin-bottom: 8px;
}
.memberInfo .info-item {
  display: flex;
  flex-flow: row nowrap;
  background: #fff;
  margin-bottom: 1px;
  padding: 20rpx 15px;
}
.lt, .rt {
  line-height: 1.4rem;
}
.info-item .lt {
  flex: 0 0 20%;
}
.info-item .rt {
  flex: auto;
  text-align: right;
}
.avatarImg, .avatarTxt {
  vertical-align: middle;
  margin-right: 10rpx;
}
.rt .avatarImg {
  height: 1.4rem;
  width: 1.4rem;
}
.rt .avatarTxt {
  display: inline-block;
  color: #666;
}
.complete {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
}
.complete button {
  width: 100%;
  height: 80rpx;
  font-size: 28rpx;
  line-height: 80rpx;
  z-index: 1;
  border-radius: 0;
}