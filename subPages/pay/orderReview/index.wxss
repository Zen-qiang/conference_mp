page {
  /* padding-bottom: calc(50px + constant(safe-area-inset-bottom));
  padding-bottom: calc(50px + env(safe-area-inset-bottom)); */
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.errorToptips {
  font-size: 28rpx !important;
}
.selectAddress_box {
  padding-bottom: 15px;
  background-color: #fff;
  position: relative;
}
.selectAddress_box::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 3px;
  background-image: linear-gradient(to right, #45b6e7, #45b6e7 10px, transparent 10px, transparent 20px, #f88d8f 20px, #f88d8f 30px, transparent 30px, transparent 40px);
  background-position: left bottom;
  background-size: 40px 3px;
  background-repeat: repeat-x;
}
.selectAddress_box, .service_box, .pay_box, .comment_box, .price_box {
  margin-bottom: 20rpx;
}
.service_box .service_time {
  color: #3695e3;
}
.service_box .orderItemList_title {
  overflow: hidden;
  white-space: nowrap;
}
.service_box .orderItemList_value {
  flex: none;
}
.service_box .orderItemList {
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
}
.pay_box .black_color {
  color: #000;
}
.pay_box .price_color {
  color: #ec132d;
}
.price_box .black_color {
  color: #000;
}

.submit-box {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  min-height: 50px;
  z-index: 3;
  display: flex;
  background-color: #fff;
  flex-flow: row nowrap;
  justify-content: flex-end;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.submit-box .content {
  flex: 1;
  text-align: right;
  padding-right: 10px;
}
.submit-box .content .text {
  display: inline-block;
  font-size: 28rpx;
  margin-right: 10rpx;
}
.submit-box .content .price {
  display: inline-block;
  font-size: 32rpx;
  color: #ec132d;
  font-weight: 400;
}
.submit-box .content .price .currency {
  font-size: 24rpx;
}
.submit-box .content .price .small {
  font-size: 24rpx;
}
.submit-box .btn {
  margin: 4px 0;
}
.submit-box .btn-submit {
  width: 100px;
}

.coupon-container {
  padding: 30rpx 20rpx 40rpx 20rpx;
  position: relative;
  background: #f5f5f5;
}
.coupon-container .nouseCoupon {
  padding: 20rpx;
  text-align: center;
  color: #3695e3;
  font-size: 28rpx;
  font-weight: 400;
}

.coupon-loading {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.coupon-loading > text {
  color: #666;
  font-size: 24rpx;
}

.coupon-container .coupon-item {
  background: #fff;
  border-radius: 4px;
  margin-bottom: 20rpx;
  display: flex;
  flex-flow: row nowrap;
  font-size: 24rpx;
  position: relative;
}
.coupon-container .coupon-item.used {
  color: #666;
}

.coupon-item .lt {
  flex: none;
  text-align: center;
  padding: 30rpx 20rpx;
}
.lt .price {
  color: #3695e3;
}
.lt .price.used {
  color: #666;
}
.lt .price .amt {
  font-size: 56rpx;
  font-weight: 500;
}
.lt .desc {
  color: #666;
}

.coupon-item .divider {
  width: 25rpx;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  position: relative;
  background: linear-gradient(to top, #f5f5f5, #f5f5f5 6px, transparent 4px, transparent) repeat-y center;
  background-size: 2px 10px;
}
.coupon-item .divider::before, .coupon-item .divider::after {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  margin: 0 auto;
  width: 25rpx;
  height: 15rpx;
  background: #f5f5f5;
}
.coupon-item .divider::before {
  top: -30rpx;
  border-radius: 0 0 15rpx 15rpx;
}
.coupon-item .divider::after {
  bottom: -30rpx;
  border-radius: 15rpx 15rpx 0 0;
}

.coupon-item .rt {
  flex: auto;
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-between;
  padding: 30rpx 20rpx;
}
.rt .title {
  font-weight: 400;
  font-size: 28rpx;
}
.rt .label {
  color: #999;
}

.coupon-item .aside {
  position: absolute;
  right: 20rpx;
  bottom: 30rpx;
}
.aside .status {
  color: #3695e3;
  padding-right: 18rpx;
  position: relative;
}
.aside .status::after {
  content: '';
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  width: 14rpx;
  height: 14rpx;
  margin: auto 0;
  background: linear-gradient(to top, #3695e3, #3695e3) no-repeat, linear-gradient(to right, #3695e3, #3695e3) no-repeat;
  background-size: 14rpx 1px, 1px 14rpx;
  transform: rotate(135deg);
}

.time-title {
  position: absolute;
  top: 0;
  height: 40px;
  line-height: 40px;
  font-size: 12px;
  color: #666;
  left: 0;
  right: 0;
  text-align: center;
}